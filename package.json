{
   "name": "my-app",
   "version": "0.0.0",
   "description": "This app is mine and for me only.",
   "main": "src/server.ts",
   "scripts": {
      "fix": "eslint src --ext .ts,.tsx --fix && prettier --write .",
      "db:generate": "drizzle-kit generate",
      "db:migrate": "mkdir -p data && drizzle-kit migrate && echo \"\n\"",
      "db:studio": "drizzle-kit studio",
      "dev:server": "tsnd --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
      "dev:css": "tailwindcss -i ./src/main.css -o ./public/main.css --watch",
      "dev:client": "tsc --project tsconfig.client.json --watch",
      "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:css\" \"pnpm dev:client\"",
      "start": "node dist/server.js",
      "build": "bash scripts/build.sh",
      "test": "echo \"Error: no test specified\" && exit 1"
   },
   "keywords": [],
   "author": "Bryan Hadinata",
   "license": "ISC",
   "packageManager": "pnpm@10.15.1",
   "dependencies": {
      "better-sqlite3": "12.2.0",
      "drizzle-orm": "0.44.5",
      "drizzle-zod": "0.8.3",
      "ejs": "^3.1.10",
      "express": "5.1.0",
      "htmx.org": "^2.0.7",
      "zod": "4.1.9"
   },
   "devDependencies": {
      "@eslint/js": "9.35.0",
      "@eslint/json": "0.13.2",
      "@tailwindcss/cli": "^4.1.13",
      "@types/better-sqlite3": "7.6.13",
      "@types/express": "5.0.3",
      "@types/jest": "30.0.0",
      "@types/node": "24.3.1",
      "@typescript-eslint/eslint-plugin": "8.43.0",
      "@typescript-eslint/parser": "8.43.0",
      "concurrently": "9.2.1",
      "dotenv": "17.2.2",
      "drizzle-kit": "0.31.4",
      "eslint": "9.35.0",
      "eslint-config-prettier": "10.1.8",
      "globals": "16.4.0",
      "jest": "30.1.3",
      "jiti": "2.5.1",
      "prettier": "3.6.2",
      "prettier-plugin-tailwindcss": "^0.6.14",
      "tailwindcss": "^4.1.13",
      "ts-jest": "29.4.1",
      "ts-node": "10.9.2",
      "ts-node-dev": "2.0.0",
      "tsc-alias": "1.8.16",
      "tsconfig-paths": "4.2.0",
      "typescript": "5.9.2",
      "typescript-eslint": "8.43.0"
   }
}
