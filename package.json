{
   "name": "my-app",
   "version": "0.0.0",
   "description": "This app is mine and for me only.",
   "main": "src/server/server.ts",
   "scripts": {
      "fix": "eslint src --ext .ts,.tsx --fix && prettier --write .",
      "db:generate": "drizzle-kit generate",
      "db:migrate": "mkdir -p data && drizzle-kit migrate",
      "db:studio": "drizzle-kit studio",
      "dev:server": "nodemon",
      "dev:css": "tailwindcss -i ./src/client/css/main.css -o ./public/main.css --watch",
      "dev:client": "vite",
      "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:css\" \"pnpm dev:client\"",
      "start": "cross-env PORT=5000 DB_FILE_NAME=test.db node dist/server.js",
      "build": "scripts/build.sh",
      "test": "jest"
   },
   "keywords": [],
   "author": "Bryan Hadinata",
   "license": "ISC",
   "packageManager": "pnpm@10.15.1",
   "dependencies": {
      "drizzle-orm": "0.44.5",
      "drizzle-zod": "0.8.3",
      "ejs": "^3.1.10",
      "express": "5.1.0",
      "htmx.org": "^2.0.7",
      "pg": "^8.18.0",
      "zod": "4.1.9"
   },
   "devDependencies": {
      "@eslint/js": "9.35.0",
      "@eslint/json": "0.13.2",
      "@tailwindcss/cli": "^4.1.13",
      "@types/express": "5.0.3",
      "@types/jest": "30.0.0",
      "@types/node": "24.3.1",
      "@types/pg": "^8.16.0",
      "@typescript-eslint/eslint-plugin": "8.43.0",
      "@typescript-eslint/parser": "8.43.0",
      "concurrently": "9.2.1",
      "cross-env": "10.1.0",
      "drizzle-kit": "0.31.4",
      "eslint": "9.35.0",
      "eslint-config-prettier": "10.1.8",
      "globals": "16.4.0",
      "jest": "30.1.3",
      "nodemon": "^3.1.11",
      "prettier": "3.6.2",
      "prettier-plugin-tailwindcss": "^0.6.14",
      "tailwindcss": "^4.1.13",
      "ts-jest": "29.4.1",
      "tsx": "^4.21.0",
      "typescript": "5.9.2",
      "typescript-eslint": "8.43.0",
      "vite": "^7.3.1"
   }
}
